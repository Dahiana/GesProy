<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - GesProy</title>
    @Styles.Render("~/Content/plantillagesproy/style.css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div id="art-main">
        <div class="art-sheet clearfix">
            <header class="art-header">

                <div class="art-shapes">
                    <div class="art-object345940188" data-left="83.95%"></div>
                </div>
                <h1 class="art-headline" data-left="38.33%">
                    <a href="#">Gestión y monitoreo de proyectos </a>
                    <a>Usuario: @Session["LogedUserID"].ToString()</a>
                </h1>
                <h2 class="art-slogan" data-left="40.82%">GesProy</h2>
            </header>
            <nav class="art-nav">
                <ul class="art-hmenu">
                    <li class="active1">@Html.ActionLink("Inicio", "Index", "Proyecto")</li>
                    <li class="active1">@Html.ActionLink("Buscar Proyectos", "Index", "Proyecto")</li>
                    <li class="active1">@Html.ActionLink("Documentación/Ayuda", "Index", "Proyecto")</li>
                    <li class="active1">@Html.ActionLink("Cerrar Sesión", "Login", "InicioSesion")</li>
                </ul>
                </nav>
            <nav class="art-nav">
                <ul class="art-hmenu">
                    <li class="active">@Html.ActionLink("Descripción General", "Index", "DescripcionGeneral")</li>
                    <li class="active">@Html.ActionLink("Entidad Ejecutora", "Index", "EntidadEjecutora")</li>
                    <li class="active">@Html.ActionLink("Cumplimiento Requisitos", "Index", "CumplimientoRequisitos")</li>
                    <li class="active">@Html.ActionLink("Fuente financiacion", "Index", "FuenteFinanciacion")</li>
                    <li class="active">@Html.ActionLink("Incorporación Presupuestal", "Index", "IncorporacionPresupuestal")</li>
                    <li class="active">@Html.ActionLink("Planeación Ejecución", "Index", "PlaneacionEjecucion")</li>
                    <li class="active">@Html.ActionLink("Fuente Gestión Precontractual", "Index", "GestionPrecontractual")</li>
                    <li class="active">@Html.ActionLink("ContratosProyecto", "Index", "ContratosProyecto")</li>
                    <li class="active">@Html.ActionLink("Cuentas y giros", "Index", "Cuenta")</li>
                    <li class="active">@Html.ActionLink("Indicadores y Avance ", "Index", "IndicadoresAvance")</li>
                    <li class="active">@Html.ActionLink("Aprobación y Formación", "Index", "AprobacionFormacion")</li>
                    <li class="active">@Html.ActionLink("Alertas", "Index", "Alerta")</li>
                    <li class="active">@Html.ActionLink("Usuario", "Index", "Usuario")</li>
                    <li class="active">@Html.ActionLink("Archivo", "Index", "Upload")</li>

                 </ul>
            </nav>

            <div class="art-layout-wrapper">
                <div class="art-content-layout">
                    <div class="art-content-layout-row">
                        <div class="art-layout-cell art-content">
                            <div class="art-block clearfix">
                            </div>

                            @RenderBody()
                            <article class="art-post art-article">
                                <h2 class="art-postheader"></h2>
                                <div class="art-postcontent art-postcontent-0 clearfix">
                                </div>
                            </article>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="art-footer">
            <div class="art-footer-inner">
                <p style="text-align: left;">G<span style="border-collapse: collapse;">esProy</span></p>
                <p><br></p>
            </div>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>